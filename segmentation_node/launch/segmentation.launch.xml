<launch>
  <!-- パラメータ -->
  <arg name="model_param"  default="$(find-pkg-share segmentation_node)/config/model.param.yaml"/>
  
  <!-- GNN Node -->
  <node pkg="segmentation_node" exec="segmentation_node" name="segmentation_node" output="screen">
    <param from="$(var model_param)"/>
  </node>
</launch>